{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "skipLibCheck": true,
  },
  "exclude": [
    "node_modules", 
    "public", 
    ".cache",
    "gatsby-config.js",
  ]
}
